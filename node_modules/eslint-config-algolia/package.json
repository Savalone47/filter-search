{
  "name": "eslint-config-algolia",
  "version": "13.3.0",
  "description": "Algolia's ESLint config.",
  "repository": "algolia/eslint-config-algolia",
  "scripts": {
    "test": "./scripts/test.sh",
    "release": "./scripts/release.sh",
    "lint": "eslint . --ignore-pattern sample-project",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "es6",
    "algolia"
  ],
  "main": "base.js",
  "author": "Algolia, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/algolia/eslint-config-algolia"
  },
  "renovate": {
    "extends": [
      "config:library"
    ],
    "semanticCommits": true,
    "rebaseStalePrs": true,
    "automerge": true,
    "major": {
      "automerge": false
    },
    "automergeType": "branch-push"
  },
  "homepage": "https://github.com/algolia/eslint-config-algolia",
  "devDependencies": {
    "conventional-changelog-cli": "2.0.12",
    "doctoc": "1.4.0",
    "eslint": "5.16.0",
    "eslint-config-algolia": "10.1.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-import": "2.17.1",
    "eslint-plugin-jest": "21.27.2",
    "eslint-plugin-prettier": "2.7.0",
    "json": "9.0.6"
  }
}
